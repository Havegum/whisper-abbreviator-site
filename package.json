{
  "name": "whisper-abbreviator-frontend",
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "description": "null",
  "postcss": {
    "plugins": {
      "postcss-preset-env": {
        "stage": 0
      },
      "autoprefixer": {},
      "tailwindcss": {}
    }
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid",
    "semi": true,
    "svelteIndentScriptAndStyle": false
  },
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "yarn build && ./deploy.sh",
    "deploy:s3": "BUILD_TARGET=s3 yarn build && ./deploy.sh --host s3 && bord4 integrate",
    "deploy:lisa": "yarn build && ./deploy.sh && bord4 integrate",
    "deploy:live": "yarn deploy && cat dist/index.html | pbcopy && echo 'Integration copied to clipboard'",
    "stage": "yarn stage:lisa",
    "stage:s3": "BUILD_TARGET=stage:s3 yarn build && ./deploy.sh --stage --host s3",
    "stage:lisa": "BUILD_TARGET=stage yarn build && ./deploy.sh --stage",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "yarn check && yarn run check && yarn eslint src/**/*.{ts,svelte} && prettier src --write",
    "purge": "curl -i -u \"purge:$PURGE_KEY\" -H \"Content-Type:application/json\" -X POST -d '{\"keys\": [\"bt-bord4-whisper-abbreviator-frontend\"]}' https://cachepurge-api.schibsted.tech/purge/keys"
  },
  "devDependencies": {
    "@nrk/core-datepicker": "^4.0.2",
    "@nrk/core-suggest": "^1.3.6",
    "@nrk/core-tabs": "^2.2.1",
    "@nrk/core-toggle": "^4.0.3",
    "@rollup/plugin-dsv": "^3.0.3",
    "@rollup/plugin-yaml": "^4.1.1",
    "@rollup/pluginutils": "^5.0.4",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/kit": "^1.25.1",
    "@tsconfig/svelte": "^5.0.2",
    "@types/dompurify": "^3.0.5",
    "@types/jsdom": "^21.1.4",
    "@types/lodash-es": "^4.17.10",
    "@types/string-template": "^1.0.5",
    "@types/ws": "^8.5.8",
    "@types/xml2js": "^0.4.13",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "@vanilla-extract/css": "^1.13.0",
    "@vanilla-extract/vite-plugin": "^3.9.0",
    "autoprefixer": "^10.4.16",
    "daisyui": "^4.4.23",
    "eslint": "^8.50.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-svelte": "^2.33.2",
    "isomorphic-ws": "^5.0.0",
    "jsdom": "^23.0.1",
    "lodash-es": "^4.17.21",
    "maplibre-gl": "^3.6.2",
    "marked": "^11.0.0",
    "nanoid": "^5.0.3",
    "openai": "^4.14.1",
    "postcss": "^8.4.32",
    "postcss-preset-env": "^9.1.4",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "rollup": "^3.29.3",
    "rollup-plugin-visualizer": "^5.9.2",
    "rxjs": "^7.8.1",
    "sqlite": "^5.1.1",
    "string-template": "^1.0.0",
    "svelte": "^4.2.1",
    "svelte-check": "^3.5.2",
    "svelte-preprocess": "^5.0.4",
    "svelte-splitpanes": "^0.8.0",
    "tailwindcss": "^3.4.0",
    "terser": "^5.20.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.6",
    "ws": "^8.14.2",
    "xml2js": "^0.6.2"
  },
  "dependencies": {
